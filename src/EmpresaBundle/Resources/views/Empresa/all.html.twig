{%  extends '::base.html.twig' %}


  {% block title %}Info Empresas


  {% endblock %}




{% block body %}

<div class="col-md-9">
        <h1>Empresas</h1>

        <table>
                <tr>
                        <th>#</th>
                        <th>Nombre</th>
                        <th>Telefono 1</th>
                        <th>telefono 2</th>
                        <th>Direccion</th>
                        <th>Codigo postal</th>
                        <th>Fecha insertada</th>

                </tr>
                {% for empresa in empresas %}
                        <tr>
                                <td>{{ empresa.id }}</td>
                                <td>{{ empresa.nombre }}</td>
                                <td>{{ empresa.telefono1 }}</td>
                                <td>{{ empresa.telefono2 }}</td>
                                <td>{{ empresa.direccion  }}</td>
                                <td>{{ empresa.cp }}</td>
                                <td>{{empresa.fechaCreacion | date('d-m-Y') }}</td>

                        </tr>
                {% endfor %}
        </table>
</div>

{% endblock %}
































